<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AttendencePage</servlet-name>
        <jsp-file>/AttendencePage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>AttendencePage</servlet-name>
        <url-pattern>/Attendance</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Attendenceinfo</servlet-name>
        <jsp-file>/Attendenceinfo.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Attendenceinfo</servlet-name>
        <url-pattern>/Attendanceinfo</url-pattern>
    </servlet-mapping>
</web-app>