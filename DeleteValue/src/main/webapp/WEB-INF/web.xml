<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>insert_values</servlet-name>
        <jsp-file>/insert_values.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>insert_values</servlet-name>
        <url-pattern>/insert_values</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>insert_controller</servlet-name>
        <jsp-file>/insert_controller.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>insert_controller</servlet-name>
        <url-pattern>/insert_controller</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>edit</servlet-name>
        <jsp-file>/edit.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>edit</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>edit_controller</servlet-name>
        <jsp-file>/edit_controller.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>edit_controller</servlet-name>
        <url-pattern>/edit_controller</url-pattern>
    </servlet-mapping>
</web-app>